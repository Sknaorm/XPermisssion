<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="toolbar" >
        <item name="corners_radius">30dp</item>
        <item name="solid_color">@color/colorPrimaryDark</item>
        <item name="solid_text_color">#333333</item>
    </style>
    <style name="un_toolbar" >
        <item name="corners_radius">10dp</item>
        <item name="stroke_width">1dp</item>
        <item name="solid_color">#00000000</item>
        <item name="stroke_color">@color/colorAccent</item>
        <item name="solid_text_color">@color/colorAccent</item>
    </style>
</resources>
