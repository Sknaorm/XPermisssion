<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="bind_id" format="reference" />
    <attr name="shape_corners_radius" format="dimension" />
    <attr name="shape_corners_left_radius" format="dimension" />
    <attr name="shape_corners_top_radius" format="dimension" />
    <attr name="shape_corners_right_radius" format="dimension" />
    <attr name="shape_corners_bottom_radius" format="dimension" />
    <attr name="shape_solid_color" format="color" />
    <attr name="shape_gradient_start_color" format="color" />
    <attr name="shape_gradient_center_color" format="color" />
    <attr name="shape_gradient_end_color" format="color" />
    <attr name="shape_gradient_center_x" format="float" />
    <attr name="shape_gradient_center_y" format="float" />
    <attr name="shape_gradient_radius" format="dimension" />
    <attr name="shape_gradient_type" format="enum">
        <enum name="linear" value="0" />
        <enum name="radial" value="1" />
        <enum name="sweep" value="2" />
    </attr>
    <attr name="shape_gradient_angle" format="enum">
        <enum name="LEFT_RIGHT" value="0" />
        <enum name="BL_TR" value="45" />
        <enum name="BOTTOM_TOP" value="90" />
        <enum name="BR_TL" value="135" />
        <enum name="RIGHT_LEFT" value="180" />
        <enum name="TR_BL" value="225" />
        <enum name="TOP_BOTTOM" value="270" />
        <enum name="TL_BR" value="315" />
    </attr>
    <attr name="shape_stroke_color" format="color" />
    <attr name="shape_stroke_width" format="dimension" />
    <attr name="shape_stroke_dash_width" format="dimension" />
    <attr name="shape_stroke_dash_gap" format="dimension" />
    <declare-styleable name="OneSelector">
        <attr name="bind_id" />
        <attr name="isSelected" format="boolean" />
        <attr name="selected_text_color" format="color" />
        <attr name="default_text_color" format="color" />
        <attr name="selected_style" format="reference" />
        <attr name="default_style" format="reference" />
        <attr name="selected_drawable" format="reference" />
        <attr name="default_drawable" format="reference" />
    </declare-styleable>
    <declare-styleable name="OneShape">
        <attr name="bind_id" />
        <attr name="shape_corners_radius" />>
        <attr name="shape_corners_left_radius" />
        <attr name="shape_corners_top_radius" />
        <attr name="shape_corners_right_radius" />
        <attr name="shape_corners_bottom_radius" />
        <attr name="shape_solid_color" />
        <attr name="shape_gradient_start_color" />
        <attr name="shape_gradient_center_color" />
        <attr name="shape_gradient_end_color" />
        <attr name="shape_gradient_center_x" />
        <attr name="shape_gradient_center_y" />
        <attr name="shape_gradient_radius" />
        <attr name="shape_gradient_type"/>
        <attr name="shape_gradient_angle"/>
        <attr name="shape_stroke_color" />
        <attr name="shape_stroke_width" />
        <attr name="shape_stroke_dash_width" />
        <attr name="shape_stroke_dash_gap" />
    </declare-styleable>
    <declare-styleable name="State">
        <attr name="shape_corners_radius" />>
        <attr name="shape_corners_left_radius" />
        <attr name="shape_corners_top_radius" />
        <attr name="shape_corners_right_radius" />
        <attr name="shape_corners_bottom_radius" />
        <attr name="shape_solid_color" />
        <attr name="shape_gradient_start_color" />
        <attr name="shape_gradient_center_color" />
        <attr name="shape_gradient_end_color" />
        <attr name="shape_gradient_center_x" />
        <attr name="shape_gradient_center_y" />
        <attr name="shape_gradient_radius" />
        <attr name="shape_gradient_type"/>
        <attr name="shape_gradient_angle"/>
        <attr name="shape_stroke_color" />
        <attr name="shape_stroke_width" />
        <attr name="shape_stroke_dash_width" />
        <attr name="shape_stroke_dash_gap" />
    </declare-styleable>
</resources>